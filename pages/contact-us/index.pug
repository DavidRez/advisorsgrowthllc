.pages-contact(:style='{ marginTop: margin }')
  .pages-contact__sections.page-section(v-for='(section, i) in props.page_sections' :key='`contact-section-${i}`')
    HeroHome(v-if='section.acf_fc_layout === "hero_home"', :props='section')
    BlockContent(v-else-if='section.acf_fc_layout === "block_content"', :props='section')
    BlockLinks(v-else-if='section.acf_fc_layout === "block_links"', :props='section')
    BlockSeparator(v-else-if='section.acf_fc_layout === "separator"', :props='section')
    CustomContactSection(v-else-if='section.acf_fc_layout === "block_contact_form"', :props='section' :global='global')